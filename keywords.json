{
    "move": [false, "m", "move", "mov"],
    "rotate": [false, "r", "rotate", "rot", "t", "turn"],
    "forward": [false, "f", "forward", "forwards", "front"],
    "backward": [false, "b", "back", "backward", "backwards"],
    "back": [false, "b", "back"],
    "front": [false, "f", "front"],
    "right": [false, "r", "right"],
    "left": [false, "l", "left"],
    "down": [false, "d", "down", "downward", "downwards"],
    "up": [false, "u", "up", "upward", "upwards"],
    "arm": [false, "a", "arm"],
    "debug-s": [false, "dbg", "debug", "db"],
    "cm-s": [false, "cm", "centimeters", "centimeter"],
    "in-s": [false, "in", "inches", "inch"],
    "deg-s": [false, "deg", "degrees", "degree"],
    "rad-s": [false, "rad", "radians", "radian"],
    "in": [true, "$in-s"],
    "rad": [true, "$rad-s"],
    "deg": [true, "$deg-s"],
    "then": [true, "then"],
    "cm": [true, "$cm-s"],
    "debug": [true, "$debug-s"],
    "move-forward": [true, "$move", "$forward"],
    "move-backward": [true, "$move", "$backward"],
    "rotate-left": [true, ["$rotate", "$left"], ["$left", "$rotate"]],
    "rotate-right": [true, ["$rotate", "$right"], ["$right", "$rotate"]],
    "arm-front-up": [true, "$front", "$arm", "$up"],
    "arm-front-down": [true, "$front", "$arm", "$down"],
    "arm-back-up": [true, "$back", "$arm", "$up"],
    "arm-back-down": [true, "$back", "$arm", "$down"]
}